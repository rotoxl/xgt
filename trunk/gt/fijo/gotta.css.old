/* xSplitter Styles */
div.clsSplitter {
	overflow: hidden;
	visibility: hidden;
	margin: 0px; padding: 0px;
	border: none;
	
	position:absolute;
	
}
body > .clsSplitter, body > .section.clsPane {position:fixed;} /*mejor para firefox, ie > 7*/
div.ventana div.clsSplitter{position:relative}
.section.clsPane {
	position: absolute;
	overflow: auto;
	visibility: visible;
	margin: 0px; padding: 0px; border: none;
	background-color:white; 
}
.section.clsPane.autosize{
	overflow: hidden;
    position: relative;
	}
div.clsDragBar {
	position: absolute;
	overflow: hidden;
	visibility: visible;
	background-color: #C3DAF9;
	background-image: url(exp.splitter.vertical.gif);
	background-position: left center;
	border: 1px solid #98C0F4;
	width: 5px;
	cursor:col-resize;
	z-index: 2 !important;
}
div.clsSplitter.horizontal div.clsDragBar{
	cursor:ns-resize;
	background-image: url(exp.splitter.horizontal.gif);
	background-position: center top;
}
.section#listaPanelesIzq{
position: fixed;
}
