/* ///////////////////////////////////
		Recarga de aplicaciones
///////////////////////////////////// */
body#recarga {font-size:108%; margin:20px; background-color:black; color:white;}
body#recarga div.erroresArranque {display:none;}

body#recarga div#aplicacion {font-size:140%; font-weight:bold; margin-bottom:10px;}
body#recarga div.est, body#recarga div.mensaje {
	background-repeat:no-repeat;
	background-position:left center;
	padding:14px;
	}
body#recarga div span {display:block;}

body#recarga > div {
	background-repeat:no-repeat;
	background-repeat:left center;
	margin-left:30px;
	}
body#recarga div#erroresArranque span.errores {white-space:pre;}
body#recarga span.texto {
	margin-left:40px; 
	border-left:1px dotted silver;
	padding-left:5px;
	}
	
body#recarga div.mensaje.info 		{background-image:url(recarga.estado.info.png);}
body#recarga div.mensaje.alerta 	{background-image:url(recarga.estado.alerta.png);}
body#recarga div.mensaje.critico 	{background-image:url(recarga.estado.critico.png);}
	
body#recarga div span.errores, 
body#recarga div#estadisticas span.est{
	display:block;
	color:darkgray;
	
	background-repeat:no-repeat;
	background-position:left center;
	
	padding:8px;
	margin-left:60px;
	padding-left:40px;
	}

body#recarga div#estadisticas span.dic {background-image:url(recarga.dic.png);}
body#recarga div#estadisticas span.exp {background-image:url(recarga.exp.png);}
body#recarga div#estadisticas span.menu {background-image:url(recarga.menu.png);}
body#recarga div#estadisticas span.tra {background-image:url(recarga.tra.png);}
body#recarga div#estadisticas span.usu {background-image:url(recarga.usu.png);}
body#recarga div#estadisticas span.idiomas {background-image:url(recarga.idiomas.png);}
